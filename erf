local a,b,c,d,e,f,g,spawn,pcall,pairs,h,hookmetamethod=CFrame.new,Color3.new,coroutine.wrap,Drawing.new,Ray.new,task.wait,Vector2.new,spawn,pcall,pairs,Vector3.new,hookmetamethod;local i=true;local j=game:GetService("Players").LocalPlayer;local k=game:GetService("Players")local l=j:GetMouse()local m=game:GetService("Workspace").CurrentCamera;hookmetamethod=hookmetamethod;Drawing=Drawing;local n=d("Circle")n.Visible=false;n.Filled=false;n.Thickness=0.5;n.Transparency=1;n.Color=b(255, 0, 0)n.Radius=_G.fov;n.Position=g(m.ViewportSize.X/2,m.ViewportSize.Y/2)Options={Torso="UpperTorso",Head="Head"}local function o()pcall(function()local p=true;spawn(function()while p do f()n.Position=g(l.X,l.Y+36)end end)end)end;c(o)()l.KeyDown:Connect(function(q)if q==_G.toggle:lower()then if i==false then n.Color=b(255, 0, 0)i=true elseif i==true then n.Color=b(1,0,0)i=false end end end)local r=nil;r=hookmetamethod(game,"__index",function(self,s)if self==l and s=="Hit"then local t=9e9;local u=nil;if i then for v,w in pairs(k:GetPlayers())do if w~=j and w.Character and w.Character:FindFirstChild("HumanoidRootPart")and w.Character:FindFirstChild("Humanoid")and w.Character:FindFirstChild("Humanoid").Health>0 then local x=w.Character;local y=a(m.CFrame.Position,x[Options.Torso].CFrame.Position)*a(0,0,-4)local z=e(y.Position,y.LookVector*9000)local A,B=workspace:FindPartOnRayWithIgnoreList(z,{j.Character:FindFirstChild("Head")})local C=(x[Options.Torso].CFrame.Position-B).magnitude;if C<4 then local D,E=m:WorldToScreenPoint(x[Options.Torso].Position)if E then local F=(g(l.X,l.Y)-g(D.X,D.Y)).Magnitude;if F<t and F<n.Radius then t=F;u=x end end end end end end;if u~=nil and u[Options.Torso]and u:FindFirstChild("Humanoid").Health>0 then if i then local G=u[Options.Torso]local H=G.CFrame+G.Velocity*_G.prediction+h(0,-1,0)return s=="Hit"and H end end end;return r(self,s)end)
